{
  "version": 2,
  "builds": [
    {
      "src": "build",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/campaign", "dest": "/index.html" },
    { "src": "/about", "dest": "/index.html" },
    { "src": "/contacts", "dest": "/index.html" },
    { "src": "/success", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
